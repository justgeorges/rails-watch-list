
<div class="row">
  <div class="col-10">
    <h1> All Lists</h1>
  </div>
  <div class="col-2">
    <%= link_to 'New List', new_list_path, :class => "btn btn-primary" %>
  </div>
</div>
<div class='row'>
  <% @lists.each do |list| %>
    <div class='col-4'>
      <%= render list %>
    </div>
  <% end %>
</div>
