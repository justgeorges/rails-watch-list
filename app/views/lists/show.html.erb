<div class="row">
  <div class="col-10">
    <h1> <%= @list.name %> </h1>
  </div>
  <div class="col-2">
    <%= link_to 'Add Bookmark', new_list_bookmark_path(@list), :class => 'btn btn-primary'%>
  </div>
<div class="row">
  <%= @bookmark.each do |bookmark|%>
    <div class='col-3'>
      <%= render bookmark %>
    </div>
    <% end %>
</div>
